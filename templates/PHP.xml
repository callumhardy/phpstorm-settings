<templateSet group="PHP">
  <template name="du" value="dump($DATA$);" description="dump()" toReformat="false" toShortenFQNames="true">
    <variable name="DATA" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="dd" value="dd($DATA$);" description="dd()" toReformat="false" toShortenFQNames="true">
    <variable name="DATA" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="ad" value="ad($DATA$);" description="ad()" toReformat="false" toShortenFQNames="true">
    <variable name="DATA" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="foreach" value="foreach ($ITERABLE$ as $VAR_VALUE$) {&#10;    $END$&#10;}" description="foreach(iterable_expr as $value) {...}" toReformat="true" toShortenFQNames="true">
    <variable name="ITERABLE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR_VALUE" expression="phpSuggestVariableName()" defaultValue="&quot;$item&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="throw" value="throw new $EXCEPTION$($END$);" description="throw new" toReformat="true" toShortenFQNames="true">
    <variable name="EXCEPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="for" value="for ($i = 0; $i &lt; $LIMIT$; $i++)&#10;{ &#10;&#9;$END$&#10;}" description="for loop" toReformat="false" toShortenFQNames="true">
    <variable name="LIMIT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="switch" value="switch ($VAR$)&#10;{&#10;&#9;case '$CASE$':&#10;&#9;&#10;&#9;&#9;$CODE$&#10;&#9;&#9;break;&#10;&#9;&#10;&#9;default:&#10;&#9;&#10;&#9;&#9;$END$&#10;&#9;&#9;break;&#10;}" description="switch statment" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CASE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CODE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="try" value="try&#10;{&#10;&#9;$TRY$&#10;}&#10;catch ($EXCEPTION$ $e)&#10;{&#10;&#9;$END$&#10;}" description="try catch" toReformat="false" toShortenFQNames="true">
    <variable name="TRY" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="EXCEPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="if" value="if ($CONDITION$)&#10;{&#10;&#9;$END$&#10;}" description="if( condition ) { code }" toReformat="false" toShortenFQNames="true">
    <variable name="CONDITION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="ifelse" value="if ($CONDITION$)&#10;{&#10;&#9;$CODE$&#10;}&#10;else&#10;{&#10;    $END$&#10;}" description="if( condition ) { code } else { code }" toReformat="false" toShortenFQNames="true">
    <variable name="CONDITION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CODE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="ife" value="if($CONDITION)&#10;{&#10;    $END$&#10;}" description="ifelse( condition ) { code }" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="vd" value="var_dump($END$);" description="var_dump()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="pp" value="echo &quot;&lt;pre&gt;&quot;;&#10;print_r($DATA$);&#10;echo &quot;&lt;/pre&gt;&quot;;" description="&lt;pre&gt; print_r() &lt;/pre&gt;" toReformat="false" toShortenFQNames="true">
    <variable name="DATA" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="pv" value="echo &quot;&lt;pre&gt;&quot;;&#10;var_dump($DATA$);&#10;echo &quot;&lt;/pre&gt;&quot;;" description="&lt;pre&gt; var_dump() &lt;/pre&gt;" toReformat="false" toShortenFQNames="true">
    <variable name="DATA" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>